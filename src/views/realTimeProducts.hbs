<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{title}}</title>
  </head>
  <body>
    <div>
      <h1>Agregar Productos</h1>
      <input type="text" name="title" id="title" placeholder="Nombre" />
      <input
        type="test"
        name="description"
        id="description"
        placeholder="Descripción"
      />
      <input type="text" name="code" id="code" placeholder="Codigo" />
      <input type="text" name="price" id="price" placeholder="Precio" />
      <input type="text" name="status" id="status" placeholder="Estado" />
      <input type="text" name="stock" id="stock" placeholder="Stock" />
      <input
        type="text"
        name="category"
        id="category"
        placeholder="Categoría"
      />
      <input type="text" name="img" id="img" placeholder="Imagen" />
      <button id="addBtn">Add product</button>

    </div>

    <div>
      <h2>Lista de Productos</h2>
      <ul id="productsList"></ul>
    </div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="/realTimeProducts.js"></script>
  </body>
</html>